#, no-c-format
msgid "<entry>id1</entry>"
msgstr "<entry>id1</entry>"

#. Tag: entry
#, no-c-format
msgid "<entry>id2</entry>"
msgstr "<entry>id2</entry>"

#. Tag: entry
#, no-c-format
msgid "Meaning"
msgstr "Betydelse"

#. Tag: entry
#, no-c-format
msgid "coincident nodes"
msgstr "sammanfallande noder"

#. Tag: entry
#, no-c-format
msgid "Identifier of first node."
msgstr "Identifierare för första noden."

#. Tag: entry
#, no-c-format
msgid "Identifier of second node."
msgstr "Identifierare för den andra noden."

#. Tag: entry
#, no-c-format
msgid "Two nodes have the same geometry."
msgstr "Två noder har samma geometri."

#. Tag: entry

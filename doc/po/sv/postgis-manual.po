msgid "4BUI - 4-bit unsigned integer"
msgstr "4BUI - 4-bitars osignerat heltal"

#. Tag: para
#, no-c-format
msgid "8BSI - 8-bit signed integer"
msgstr "8BSI - 8-bitars signerat heltal"

#. Tag: para
#, no-c-format
msgid "8BUI - 8-bit unsigned integer"
msgstr "8BUI - 8-bitars osignerat heltal"

#. Tag: para
#, no-c-format
msgid "16BSI - 16-bit signed integer"
msgstr "16BSI - 16-bitars signerat heltal"

#. Tag: para
#, no-c-format
msgid "16BUI - 16-bit unsigned integer"
msgstr "16BUI - 16-bitars osignerat heltal"

#. Tag: para
#, no-c-format
msgid "32BSI - 32-bit signed integer"
msgstr "32BSI - 32-bitars signerat heltal"

#. Tag: para
#, no-c-format
msgid "32BUI - 32-bit unsigned integer"
msgstr "32BUI - 32-bitars osignerat heltal"

#. Tag: para
#, no-c-format
msgid "32BF - 32-bit float"
msgstr "32BF - 32-bitars float"

#. Tag: para
#, no-c-format
msgid "64BF - 64-bit float"
msgstr "64BF - 64-bitars float"

#. Tag: refpurpose
#, no-c-format
msgid ""
"<refpurpose>Returns the minimum value this pixeltype can store.</refpurpose>"
msgstr ""
"<refpurpose>Returnerar det lägsta värde som denna pixeltyp kan lagra.</"
"refpurpose>"

#. Tag: para
#, no-c-format
msgid "<para>Returns the minimum value this pixeltype can store.</para>"
msgstr "<para>Returnerar det lägsta värde som denna pixeltyp kan lagra.</para>"

